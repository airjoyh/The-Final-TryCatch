<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="example">
	<insert id="create">
		insert into 
		example(item_no, item, quiz_id)
		values(#{item_no}, #{item}, #{quiz_id})
	</insert>
	
